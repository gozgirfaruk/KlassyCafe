
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">

   @await Html.PartialAsync("/Views/Shared/_AdminLayoutPartial/_HeadPartial.cshtml")

<body>
    <div id="preloader">
        <div class="loader">
            <svg class="circular" viewBox="25 25 50 50">
                <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
            </svg>
        </div>
    </div>

    <div id="main-wrapper">

        <div class="nav-header">
            <div class="brand-logo">
                <a href="index.html">
                    <b class="logo-abbr"><img src="images/logo.png" alt=""> </b>
                    <span class="logo-compact"><img src="~/quixlab-master/images/logo-compact.png" alt=""></span>
                    <span class="brand-title">
                        <img src="~/quixlab-master/images/logo-text.png" alt="">
                    </span>
                </a>
            </div>
        </div>
    
        <div class="header">
            <div class="header-content clearfix">

                <div class="nav-control">
                    <div class="hamburger">
                        <span class="toggle-icon"><i class="icon-menu"></i></span>
                    </div>
                </div>
                <div class="header-left">
                    <div class="input-group icons">
                        <div class="input-group-prepend">
                            <span class="input-group-text bg-transparent border-0 pr-2 pr-sm-3" id="basic-addon1"><i class="mdi mdi-magnify"></i></span>
                        </div>
                        <input type="search" class="form-control" placeholder="Search Dashboard" aria-label="Search Dashboard">
                        <div class="drop-down animated flipInX d-md-none">
                            <form action="#">
                                <input type="text" class="form-control" placeholder="Search">
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
   
        @await Html.PartialAsync("/Views/Shared/_AdminLayoutPartial/_SidebarPartial.cshtml")
  
        <div class="content-body">

            @RenderBody()
         
        </div>
    </div>

    @await Html.PartialAsync("/Views/Shared/_AdminLayoutPartial/_ScriptPartial.cshtml")

</body>
</html>
